    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="11"
        android:targetSdkVersion="17" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.invoiceapp.LoginScreenActivity"
            android:label="@string/app_name" ><intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="BreadListActivity"></activity>
        <activity android:name="PreviewBreadListActivity"></activity>
        <activity android:name="OrderConfirmationActivity"></activity>
        <activity android:name="CustomerListActivity"></activity>
        <activity android:name="CustomerActivity"></activity>
    </application>

</manifest>
